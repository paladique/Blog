<footer id="footer">
  <div class="footer-left">
    Copyright &copy; <%= new Date().getFullYear() %> <%= config.author || config.title %>
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav) { %>
          <li><a href="<%- url_for(theme.nav[i]) %>"><%= i %></a></li>
        <% } %>
        <li>
            <% if (theme.customize.social_links) { %>
                <% for(var i in theme.customize.social_links) { %>
                  <a class="icon" target="_blank" href="<%- url_for(theme.customize.social_links[i]) %>" title="<%= i %>"><i class="fa fa-<%= i %>"></i></a>
                <% } %>
            <% } %>
        </li>
      </ul>
    </nav>
  </div>
</footer>
